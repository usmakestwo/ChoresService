  "rootPackage": "com.usmakestwo.chores",
  "handlerPackage":"com.usmakestwo.chores.handler",
  "modelPackage":"com.usmakestwo.chores.model",
  "overwriteHandler": true,
  "overwriteHandlerTest": true,
  "overwriteModel": true,
  "httpPort": 8080,
